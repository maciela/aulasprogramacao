# Lendo X e Y e trocando seus valores entre si
X = float(input("Entre com um valor para X:"))
Y = float(input("Entre com um valor para Y:"))
X2 = Y
Y2 = X
print(" O valor digitado para X é: ",X)
print(" O valor digitado para Y é: ",Y)

print("O novo valor para X é: ", X2)
print("O novo valor para Y é: ", Y2)
